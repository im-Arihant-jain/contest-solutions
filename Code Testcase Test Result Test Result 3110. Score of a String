class Solution {
public:
    int scoreOfString(string s) {
        int answ=0;
        int length=s.length();
        for(int i=0;i<(length-1);i++)
        {
            int a=(int)s[i];
            int b=(int)s[i+1];
            answ=answ+(abs(b-a));
         
        }
        return answ;
    }
};
